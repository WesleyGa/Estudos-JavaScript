function compras(trabalho1, trabalho2) {

    televisao50 = trabalho1 && trabalho2;
    tomarSorvete = trabalho1 || trabalho2;
    // const comprarTv32 = !!(trabalhoA ^ trabalhoB) // bitwise xor
    televisao32 = trabalho1 != trabalho2;
    manterSaudavel = !tomarSorvete;

    return { tomarSorvete, televisao32, televisao50, manterSaudavel };
}
console.log(compras(false, false));
console.log(compras(true, false));
console.log(compras(true, true));
console.log(compras(false, true));